.fs_color_red{
	color: #E72E2D;
}
.fs_color_gray9{
	color: #999999;
}
.flex_box{
    display: flex;
 }
.column_flex_box{
	display: flex;
	flex-direction: column;
}
.text_hidden_{
    white-space: nowrap;
    overflow: hidden;
}
.text_hidden_ellipsis{
    text-overflow:ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.pubTextTwohidden{
	overflow:hidden; 
	text-overflow:ellipsis;
	display:-webkit-box;
	-webkit-line-clamp:2;
	-webkit-box-orient: vertical;
	line-height: 1.2;
}
.pub_input_box{
	position: relative;
}
.pub_input_box input{
	display: inline-block;
    border-radius: 80rpx;
    background: #F5F5F5;
	font-size: 26rpx;
    min-height: 30rpx!important;
    height: 30rpx!important;
    overflow: visible;
}
.pub_input_box .search_img{
	width: 26rpx;
    position: absolute;
    left: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}
/* 4s */
.order_title{
	font-size: 32rpx;
	color: #333333;
	display: flex;
	align-items: center;
}
.order_title .line_{
	width: 2rpx;
	height: 25rpx;
	margin-right: 16rpx;
	background: #567BFF;
}
/* swiper的dot */
.pub_swiper_dot .wx-swiper-dots-horizontal{
    margin-bottom: 2rpx;
}
.pub_swiper_dot .wx-swiper-dot{
    display: inline-flex;
    width: 56rpx;
    height: 8rpx;
    margin-left: 24rpx;
    justify-content:space-between;
}
.pub_swiper_dot .wx-swiper-dot::before{
    content: '';
    flex-grow: 1; 
    background: #F4F4F4;
    border-radius: 8rpx
}
.pub_swiper_dot .wx-swiper-dot-active::before{
    background:#567BFF;   
}
.go_rise_level_box_pub{
	background: white;
	padding: 20rpx 50rpx;
	margin-top: 20rpx;
}
.go_rise_level_box_pub .img_{
	width: 100%;
}
.label_title_tpl_title{
    font-size: 28rpx;
	color: #666666;
	text-align: center;
	padding-bottom: 18rpx;
}
.label_title_tpl_title .img_l{
    width: 92rpx;
}
.label_title_tpl_title .dot{
	display: inline-block;
	width: 7rpx;
	height: 7rpx;
	background:#666666;
	border-radius: 100%;
	vertical-align: middle;
	margin: auto 20rpx;
}
/* 操作按钮 */
.option_btn{
	width: 706rpx;
	padding: 30rpx;
	font-size: 36rpx;
	color: white;
	font-weight: bold;
	text-align: center;
	border-radius: 10rpx;
	background: #567BFF;
	margin: 157rpx auto 60rpx;
}
/* 底部按钮 */
.bottom_btn_pub{ /*1个*/
    position: fixed;
    bottom: 0;
    width: 100%;
}
.fixed_pub_body{ /* adress */
	width: 100%;
    height: 100%;
    overflow: auto;
}
.fixed_pub_body .detail_box{/* adress */
	width: 100%;
    height: 100%;
    overflow: auto;
    padding-bottom: 200rpx;
}
.bottom_more_btn_pub{/*2个*/
	position: fixed;
    width: 100%;
    display: flex;
    bottom: 0;
    font-size: 32rpx;
    color: white;
}
.bottom_more_btn_pub .btn{
    flex: 1;
    padding: 34rpx;
    text-align: center;
}
.bottom_more_btn_pub .blue_bg{
    background: #567BFF;
}
.bottom_more_btn_pub .gray_bg{
    background: #CCCCCC;
}
/* 加减数量 */
.pub_option_num_box{
    display: flex;
    justify-content: center;
    align-items: flex-start;
}
 .pub_option_num_box .opt_{
    font-size: 36rpx;
}
 .pub_option_num_box .opt_.cant{
    color: #999999;
}
 .pub_option_num_box .number_{
    font-size: 24rpx;
    background: #F4F4F4;
    padding: 10rpx 36rpx;
    margin: 0 20rpx;
}
/* 商品详情 */
.pub_order_goods_detail{
    display: flex;
    justify-content: space-between;
}
.pub_order_goods_detail .goods_img{
    width: 128rpx;
    height: 128rpx;
    margin-left: 7rpx;
}
.pub_order_goods_detail .goods_info_box{
    margin: auto 16rpx ;
    height: 128rpx;
    max-width: 442rpx;
    align-self: center;
    font-size: 28rpx;
    display: flex;
    flex-direction: column;
	justify-content: space-around;
	color: #666666;
}
.pub_order_goods_detail .goods_info_box .integral_{
    color: #FE1111;
}
.pub_order_goods_detail .num_{
    font-size: 28rpx;
}
/* tab */
.pub_tab_box{
    display: flex;
    background: white;
}
.pub_tab_box .tab_{
	flex: 1;
	text-align: center;
    color: #999999;
	font-size: 28rpx;
	overflow: hidden;
}
.pub_tab_box .tab_ .text_{
    padding: 21rpx 22rpx;
    display: inline-block;
}
.pub_tab_box .tab_.active_ .text_{
    color: #567BFF;
    border-bottom: 2rpx solid currentColor;
}
.mask_box{
	width:100%;
	height:100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index:999;
	background-color:rgba(15, 15, 26, 0.7);
}
  .line_right{
	  position: relative;
	  margin-right: 15rpx;
  }
  .line_right:after{
	  content: '';
	  position: absolute;
	  display: inline-block;
	  width: 1px;
	  height: 50%;
	  top: 30%;
	  right: -10rpx;
	  background: #e2e2e2;
  }
  .first_{
	  color: #f9e9b5;
	  background: #fea531;
	  padding: 0 5rpx;
  }
  /* 底部上线 */
  .top_line_shaw{
	  position: relative;
  }
  .top_line_shaw:before{
	  display: inline-block;
	  position: absolute;
	  content: '';
	  width: 100%;
	  height: 0;
	  box-shadow: 0 0 1px 1px #efefef;
	  top: 0;
	  left: 0;
	  z-index: 2;
  }
/* 蒙层 */
