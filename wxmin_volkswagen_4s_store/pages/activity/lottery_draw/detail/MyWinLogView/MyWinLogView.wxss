
/* 我的奖励列表 */
.win_log_box .title_{
    text-align: center;
    margin-bottom: 50rpx;
}
.win_log_box .title_img_{
    width: 625rpx;
}
.win_log_box .detail_{
    margin: 0 10rpx 80rpx;
    position: relative;
}
.win_log_box .detail_ .bg_img_{
    width: 100%;
    height: 701rpx;
}
.win_log_box .log_box{
    position: absolute;
    width: 100%;
    height: 100%;
    overflow: auto;
    top: 0;
    left: 0;
    padding: 0 68rpx;
}
.win_log_box .log_box .tab_th{
    display: flex;
    justify-content: space-between;
    padding: 62rpx 43rpx 44rpx 33rpx;
    border-bottom: 4rpx solid #999999;
    font-size: 32rpx;
}
.win_log_box .log_box .tab_th .time_{
   color: #567BFF;
}
.win_log_box .log_box .tab_th .val_{
   color: #FF0000;
}
.win_log_box .log_box .log_detail_box{
    max-height: 550rpx;
    overflow: auto;
    font-size: 24rpx;
    color: #666666;
}
.win_log_box .log_box .log_detail_box .log_{
    display: flex;
    justify-content: space-between;
    padding: 40rpx 5rpx 40rpx;
    border-bottom: 4rpx solid #999999;
}
.win_log_box .log_box .log_detail_box .win_val{
    position: relative;
}
.win_log_box .log_box .log_detail_box .had_sent_recode{
    position: absolute;
    display: inline-block;
    width: 92rpx;
    top: 50%;
    left: 50%;
    transform: translateY(-50%) translateX(-50%);
}
.win_log_box .log_box .log_detail_box .get_integral{
    margin-right: 52rpx;
}
.win_log_box .log_box .log_detail_box .get_other{
    color: #FC0808;
}
.win_log_box .log_detail_box .log_:last-child{
    border: 0;
}
