.search_goods_body{
    background: white;
    width: 100%;
    height: 100%;
    overflow: auto;
}
.search_goods_body .search_box{
    display: flex;
    color: #999999;
    font-size: 26rpx;
    position: relative;
    align-items: center;
    padding: 30rpx 22rpx 26rpx;
}
.search_goods_body .search_box .input_box .img_{
    width: 26rpx;
    position: absolute;
}
.search_goods_body .search_box .input_box input{
    background: #F5F5F5;
    padding: 15rpx 57rpx 18rpx;
    width: 450rpx;
    margin-right: 23rpx;
}
.search_goods_body .history_list_box .title_{
    display: flex;
    justify-content: space-between;
    padding: 20rpx 40rpx 20rpx 24rpx;
    background: #F4F8FB;
    color: #999999;
    font-size: 32rpx;
}
.search_goods_body .history_list_box .title_ .img_{
    width: 32rpx;
}
.search_goods_body .history_list_box .detail_box{
    padding: 0 22rpx 20rpx;
    display: flex;
    flex-wrap: wrap;
}
.search_goods_body .history_list_box .detail_box .detail_{
    font-size: 28rpx;
    padding: 20rpx 14rpx;
    background: #F4F8FB;
    margin-right: 30rpx;
    margin-top: 20rpx;
    border-radius: 4rpx;
}