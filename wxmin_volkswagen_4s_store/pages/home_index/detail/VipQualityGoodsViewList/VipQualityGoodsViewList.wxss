.home_vip_goods_body .detail_box{
    display: flex;
    flex-wrap: wrap;
    padding: 24rpx;
    justify-content: space-between;
}

.detail_box_one_item {
  display: flex;
  flex-direction: column;
  background: #fff;
  width: 344rpx;
  border-radius:6px;
  margin-bottom: 16rpx;
}

.home_vip_goods_body .detail_box .detail_box_one_img {
    width: 343rpx;
    height: 333rpx;
    border-top-left-radius: 6rpx;
    border-top-right-radius: 6rpx;
}
.detail_box_one_title{
    font-size:29rpx;
    font-family:PingFang SC;
    font-weight:bold;
    color:rgba(46,46,46,1);
    padding-left: 10rpx;
    padding-right: 20rpx;
    padding-top: 20rpx;
    display: -webkit-box;
    -webkit-line-clamp:2;
    -webkit-box-orient: vertical;
    overflow: hidden
}
.detail_box_one_money {
   font-family:PingFang SC;
  font-weight:bold;
  color:rgba(86,123,255,1);
  padding: 10rpx;
}
.detail_box_one_money_unit {
   font-size:29rpx;
}
.detail_box_one_money_value {
  font-size:38rpx;
}
.vip_search_bar {
    padding: 17rpx 90rpx;
    background:rgba(86,123,255,1);
    position: relative;
}

.vip_search_icon {
  position: absolute;
  top: 50%;
  left: 107rpx;
  transform: translateY(-50%);
}
.search_placeholder_style {
  font-size:25rpx;
  color:rgba(130,129,127,1);
}

.vip_search_bar_input {
   font-size:25rpx;
   font-family:PingFang;
   font-weight:500;
   color:rgba(38,38,38,1);
   background:rgba(239,238,236,1);
   border-radius:28rpx; 
   padding: 10rpx 10rpx 10rpx 79rpx;
   position: relative;
}

.vip_tab_view {
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #fff;
  overflow-x: auto;
  box-sizing: border-box;
}
.justify-center {
  justify-content: center;
}
.justify-start {
  justify-content: start;
}

.vip_tab  {
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:800;
  color:rgba(38,38,38,1);
  padding: 20rpx 10rpx;
  text-align: center;
  width: 160rpx;
  flex-shrink: 0;
}
.active_vip_tab {
  color:rgba(86,123,255,1);
  border-bottom: 4rpx solid  rgba(86,123,255,1);
}

.vip_seperator_line {
  width: 1rpx;
  min-width: 1rpx;
  height: 30rpx;
  background: #BFBFBF;
  margin-right: 11rpx;
  margin-left: 11rpx;
}
